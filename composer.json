{
  "name": "survos/location-bundle",
  "type": "symfony-bundle",
  "description": "Use Geonames to in an application",
  "keywords": ["city", "state"],
  "homepage": "https://github.com/survos/SurvosLocationBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Tac Tacelosky",
      "email": "tacman@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "doctrine/orm": "^2.13",
    "doctrine/persistence": "^2.0||^3.0",
    "stof/doctrine-extensions-bundle": "^1.7",
    "survos/api-grid-bundle": "1.1.x-dev",
    "symfony/flex": "^2.0",
    "symfony/form": ">=5.4",
    "symfony/framework-bundle": ">=5.4",
    "symfony/validator": ">=5.4",
    "symfony/yaml": ">=5.4"
  },
  "require-dev": {
    "api-platform/core": "^3",
    "phpstan/phpstan": "^1.7",
    "phpunit/phpunit": "^9.5",
    "rector/rector": "^0.13.6",
    "symfony/phpunit-bridge": ">=4.0",
    "symplify/config-transformer": "^9.4"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  },
  "autoload": {
    "psr-4": {
      "Survos\\LocationBundle\\": "src"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Survos\\LocationBundle\\Tests\\": "tests/"
    }
  },
  "conflict": {
    "symfony/symfony": "<4.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
